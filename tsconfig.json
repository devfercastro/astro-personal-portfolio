{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "preact",
		"baseUrl": ".",
		"paths": {
			"@src/*": ["src/*"],
			"@components/*": ["src/components/*"],
			"@public/*": ["public/*"],
			"@icons/*": ["src/components/icons/*"],
			"@utils/*": ["src/utils/*"]
		}
	}
}
